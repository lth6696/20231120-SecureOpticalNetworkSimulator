    <!-- 'load' 业务负载 100 200 300 400 500 600 | 12 6 4 3 2.4 2-->
    <!-- 'max-rate=' 最高速率，用于结果统计 -->
    <!-- 'statisticStart' 表示将从哪个业务开始收集统计信息 -->
